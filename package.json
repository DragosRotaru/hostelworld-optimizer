{
  "name": "digital-nomad",
  "version": "1.0.0",
  "description": "Making my life easier through code",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DragosRotaru/digital-nomad.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DragosRotaru/digital-nomad/issues"
  },
  "homepage": "https://github.com/DragosRotaru/digital-nomad#readme",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "src"
  ],
  "engines": {
    "node": ">=12.8.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "tslint": {
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-jest",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {},
    "rulesDirectory": []
  },
  "scripts": {
    "start": "npm run build && node -r dotenv/config lib/index.js",
    "build": "npx tsc",
    "test": "npx jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^2.1.2",
    "@hapi/joi": "^17.1.0",
    "@types/joi": "^14.3.4",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "csv-writer": "^1.6.0",
    "fp-ts": "^2.5.3",
    "http-status-codes": "^1.4.0",
    "io-ts": "^2.1.2",
    "libphonenumber-js": "^1.7.46",
    "moment": "^2.24.0",
    "normalize-url": "^5.0.0",
    "papaparse": "^5.1.1",
    "puppeteer": "^2.1.1",
    "uuid": "^7.0.2",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/uuid": "^7.0.0",
    "@types/validator": "^12.0.1",
    "dotenv": "^8.2.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-jest": "^1.0.5",
    "typescript": "^3.8.3"
  }
}
